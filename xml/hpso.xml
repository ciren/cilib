<?xml version="1.0"?>
<!DOCTYPE simulator [
<!ATTLIST algorithm id ID #IMPLIED>
<!ATTLIST problem id ID #IMPLIED>
<!ATTLIST measurements id ID #IMPLIED>
]>
<simulator>
    <algorithms>
        <!-- Dynamic HPSO -->
        <algorithm id="dynamic-hpso" class="pso.PSO">
            <addStoppingCondition class="stoppingcondition.MaximumIterations" maximumIterations="1000"/>
            <initialisationStrategy class="algorithm.initialisation.HeterogeneousPopulationInitialisationStrategy">
                <entityNumber value="50"/>
                <entityType class="pso.particle.StandardParticle"/>
                
		<!-- initial behaviors -->
                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                            <cognitiveAcceleration class="controlparameter.LinearDecreasingControlParameter">
                                <upperBound value="2.5"/>
                                <lowerBound value="0.5"/>
                                <parameter value="2.5"/>
                            </cognitiveAcceleration>
                            <socialAcceleration class="controlparameter.LinearIncreasingControlParameter">
                                <lowerBound value="0.5"/>
                                <upperBound value="2.5"/>
                                <parameter value="0.5"/>
                            </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                            <cognitiveAcceleration class="controlparameter.ConstantControlParameter">
                                <parameter value="2.5"/>
                            </cognitiveAcceleration>
                        <socialAcceleration class="controlparameter.ConstantControlParameter">
                            <parameter value="0.0"/>
                        </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                        <cognitiveAcceleration class="controlparameter.ConstantControlParameter">
                            <parameter value="0.0"/>
                        </cognitiveAcceleration>
                            <socialAcceleration class="controlparameter.ConstantControlParameter">
                                <parameter value="2.5"/>
                            </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.BareBonesVelocityUpdateStrategy"/>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.LinearPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.BareBonesExploitVelocityUpdateStrategy"/>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.LinearPositionUpdateStrategy"/>
                </addBehavior>
		<!-- end initial behaviors -->
            </initialisationStrategy>

            <iterationStrategy class="pso.hpso.DynamicHeterogeneousIterationStrategy">
		<!-- behavior pool -->
                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                            <cognitiveAcceleration class="controlparameter.LinearDecreasingControlParameter">
                                <upperBound value="2.5"/>
                                <lowerBound value="0.5"/>
                                <parameter value="2.5"/>
                            </cognitiveAcceleration>
                            <socialAcceleration class="controlparameter.LinearIncreasingControlParameter">
                                <lowerBound value="0.5"/>
                                <upperBound value="2.5"/>
                                <parameter value="0.5"/>
                            </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                            <cognitiveAcceleration class="controlparameter.ConstantControlParameter">
                                <parameter value="2.5"/>
                            </cognitiveAcceleration>
                        <socialAcceleration class="controlparameter.ConstantControlParameter">
                            <parameter value="0.0"/>
                        </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                        <cognitiveAcceleration class="controlparameter.ConstantControlParameter">
                            <parameter value="0.0"/>
                        </cognitiveAcceleration>
                            <socialAcceleration class="controlparameter.ConstantControlParameter">
                                <parameter value="2.5"/>
                            </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.BareBonesVelocityUpdateStrategy"/>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.LinearPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.BareBonesExploitVelocityUpdateStrategy"/>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.LinearPositionUpdateStrategy"/>
                </addBehavior>
                <!-- end behavior pool -->
            </iterationStrategy>
        </algorithm>

	<!-- Adaptive HPSO -->
        <algorithm id="adaptive-hpso" class="pso.PSO">
            <addStoppingCondition class="stoppingcondition.MaximumIterations" maximumIterations="1000"/>
            <initialisationStrategy class="algorithm.initialisation.HeterogeneousPopulationInitialisationStrategy">
                <entityNumber value="50"/>
                <entityType class="pso.particle.StandardParticle"/>
                
		<!-- initial behaviors -->
                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                            <cognitiveAcceleration class="controlparameter.LinearDecreasingControlParameter">
                                <upperBound value="2.5"/>
                                <lowerBound value="0.5"/>
                                <parameter value="2.5"/>
                            </cognitiveAcceleration>
                            <socialAcceleration class="controlparameter.LinearIncreasingControlParameter">
                                <lowerBound value="0.5"/>
                                <upperBound value="2.5"/>
                                <parameter value="0.5"/>
                            </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                            <cognitiveAcceleration class="controlparameter.ConstantControlParameter">
                                <parameter value="2.5"/>
                            </cognitiveAcceleration>
                        <socialAcceleration class="controlparameter.ConstantControlParameter">
                            <parameter value="0.0"/>
                        </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                        <cognitiveAcceleration class="controlparameter.ConstantControlParameter">
                            <parameter value="0.0"/>
                        </cognitiveAcceleration>
                            <socialAcceleration class="controlparameter.ConstantControlParameter">
                                <parameter value="2.5"/>
                            </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.BareBonesVelocityUpdateStrategy"/>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.LinearPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.BareBonesExploitVelocityUpdateStrategy"/>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.LinearPositionUpdateStrategy"/>
                </addBehavior>
		<!-- end initial behaviors -->
            </initialisationStrategy>
            <iterationStrategy class="pso.hpso.AdaptiveHeterogeneousIterationStrategy">
		<!-- behavior pool -->
                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                            <cognitiveAcceleration class="controlparameter.LinearDecreasingControlParameter">
                                <upperBound value="2.5"/>
                                <lowerBound value="0.5"/>
                                <parameter value="2.5"/>
                            </cognitiveAcceleration>
                            <socialAcceleration class="controlparameter.LinearIncreasingControlParameter">
                                <lowerBound value="0.5"/>
                                <upperBound value="2.5"/>
                                <parameter value="0.5"/>
                            </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                            <cognitiveAcceleration class="controlparameter.ConstantControlParameter">
                                <parameter value="2.5"/>
                            </cognitiveAcceleration>
                        <socialAcceleration class="controlparameter.ConstantControlParameter">
                            <parameter value="0.0"/>
                        </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.StandardVelocityUpdate">
                        <cognitiveAcceleration class="controlparameter.ConstantControlParameter">
                            <parameter value="0.0"/>
                        </cognitiveAcceleration>
                            <socialAcceleration class="controlparameter.ConstantControlParameter">
                                <parameter value="2.5"/>
                            </socialAcceleration>
                    </velocityUpdateStrategy>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.StandardPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.BareBonesVelocityUpdateStrategy"/>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.LinearPositionUpdateStrategy"/>
                </addBehavior>

                <addBehavior class="pso.particle.ParticleBehavior">
                    <velocityUpdateStrategy class="pso.velocityupdatestrategies.BareBonesExploitVelocityUpdateStrategy"/>
                    <positionUpdateStrategy class="pso.positionupdatestrategies.LinearPositionUpdateStrategy"/>
                </addBehavior>
		<!-- end behavior pool -->
            </iterationStrategy>
        </algorithm>
    </algorithms>

    <measurements id="measurements" class="simulator.MeasurementSuite" resolution="5">
        <addMeasurement class="measurement.single.Fitness"/>
        <addMeasurement class="measurement.single.diversity.Diversity"/>
        <addMeasurement class="measurement.multiple.AdaptiveHPSOBehaviorProfileMeasurement"/>
    </measurements>

    <problems>
        <problem id="Rastrigin" class="problem.FunctionMinimisationProblem">
            <domain value="R(-5.12, 5.12)^30"/>
            <function class="functions.continuous.unconstrained.Rastrigin"/>
        </problem>

        <simulation samples="30">
            <algorithm idref="dynamic-hpso"/>
            <problem idref="Rastrigin"/>
            <measurements idref="measurements"/>
	     <output format="TXT" file="data/dynamic-hpso_Rastrigin_30_Dimensions.txt"/>
        </simulation>
        <simulation samples="30">
            <algorithm idref="adaptive-hpso"/>
            <problem idref="Rastrigin"/>
            <measurements idref="measurements"/>
	     <output format="TXT" file="data/adaptive-hpso_Rastrigin_30_Dimensions.txt"/>
        </simulation>
    </problems>
</simulator>
